<h1>Register</h1>

<%= form_tag '/users' do %>
  <%= label_tag :name %>
  <%= text_field_tag :name, @form_info['name'] %>

  <%= label_tag :address %>
  <%= text_field_tag :address, @form_info['address'] %>

  <%= label_tag :city %>
  <%= text_field_tag :city, @form_info['city'] %>

  <%= label_tag :state %>
  <%= text_field_tag :state, @form_info['state'] %>

  <%= label_tag :zip %>
  <%= text_field_tag :zip, @form_info['zip'] %>

  <%= label_tag :email %>
  <%= text_field_tag :email%>

  <%= label_tag :password %>
  <%= password_field_tag :password %>

  <%= label_tag :confirm_password %>
  <%= password_field_tag :confirm_password %>

  <%= submit_tag "Create New User" %>

<% end %>
